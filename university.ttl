:alice rdf:type owl:NamedIndividual ,
                :Student ;
       :enrolledIn :ml101 ;
       :hasID "S-001" .


###  http://example.org/university#bob
:bob rdf:type owl:NamedIndividual ;
     :teaches :ml101 ;
     :hasID "P-001" .


###  http://example.org/university#carol
:carol rdf:type owl:NamedIndividual ,
                :Student ;
       :enrolledIn :ml101 ;
       :hasID "S-002" .


###  http://example.org/university#csDept
:csDept rdf:type owl:NamedIndividual ,
                 :Department ;
        :offers :ml101 .


###  http://example.org/university#ml101
:ml101 rdf:type owl:NamedIndividual ,
                :Course ;
       :credits 3 .


