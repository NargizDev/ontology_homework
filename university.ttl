<http://example.org/university> rdf:type owl:Ontology .

#################################################################
#    Object Properties
#################################################################

###  http://example.org/university#enrolledIn
:enrolledIn rdf:type owl:ObjectProperty ;
            rdfs:domain :Student ;
            rdfs:range :Course ;
            rdfs:label "enrolledIn"@en .


###  http://example.org/university#isTaughtBy
:isTaughtBy rdf:type owl:ObjectProperty ;
            owl:inverseOf :teaches ;
            rdfs:domain :Course ;
            rdfs:label "isTaughtBy"@en .


###  http://example.org/university#offeredBy
:offeredBy rdf:type owl:ObjectProperty ;
           owl:inverseOf :offers ;
           rdfs:domain :Course ;
           rdfs:range :Department ;
           rdfs:label "offeredBy"@en .


###  http://example.org/university#offers
:offers rdf:type owl:ObjectProperty ;
        rdfs:domain :Department ;
        rdfs:range :Course ;
        rdfs:label "offers"@en .


###  http://example.org/university#teaches
:teaches rdf:type owl:ObjectProperty ;
         rdfs:range :Course ;
         rdfs:label "teaches"@en .


#################################################################
#    Data properties
#################################################################

###  http://example.org/university#credits
:credits rdf:type owl:DatatypeProperty ;
         rdfs:domain :Course ;
         rdfs:range xsd:integer ;
         rdfs:label "course credits"@en .


###  http://example.org/university#hasID
:hasID rdf:type owl:DatatypeProperty ;
       rdfs:domain :Person ;
       rdfs:range xsd:string ;
       rdfs:label "string identifier"@en .


#################################################################
#    Classes
#################################################################

###  http://example.org/university#Course
:Course rdf:type owl:Class .


###  http://example.org/university#Department
:Department rdf:type owl:Class .


###  http://example.org/university#Person
:Person rdf:type owl:Class .


###  http://example.org/university#Student
:Student rdf:type owl:Class ;
         rdfs:subClassOf :Person ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :enrolledIn ;
                           owl:someValuesFrom :Course
                         ] .


#################################################################
#    Individuals
#################################################################

